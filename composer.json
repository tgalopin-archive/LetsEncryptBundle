{
    "name": "tga/letsencrypt-bundle",
    "type": "symfony-bundle",
    "description": "Free SSL/HTTPS for your Symfony application with automatic renewal and certificates monitoring",
    "keywords": [
        "symfony",
        "letsencrypt",
        "free",
        "ssl",
        "https",
        "automatic",
        "renewal",
        "certificates",
        "monitoring"
    ],
    "homepage": "https://github.com/tgalopin/LetsEncryptBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com",
            "homepage": "http://titouangalopin.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.3|~3.0",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1.4",
        "symfony/yaml": "~2.3|~3.0",
        "swiftmailer/swiftmailer": "~4.3|~5"
    },
    "autoload": {
        "psr-4": {
            "Tga\\LetsEncryptBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tga\\LetsEncryptBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
